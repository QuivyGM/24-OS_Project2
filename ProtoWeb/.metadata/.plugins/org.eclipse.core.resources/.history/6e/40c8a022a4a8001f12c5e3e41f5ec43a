package com.example.protoweb;

import java.util.List;
import java.util.Map;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.http.HttpStatus;
import org.springframework.http.ResponseEntity;
import org.springframework.stereotype.Controller;
import org.springframework.web.bind.annotation.GetMapping;
import org.springframework.web.bind.annotation.ResponseBody;
import org.springframework.web.bind.annotation.RestController;

import com.example.protoweb.Tables.Accounts;

@RestController
public class ListController {
	@Autowired
    private AccountsService Sv;
	@GetMapping("/Accounts")
	private ResponseEntity<List<Accounts>> view() {
		return new ResponseEntity<List<Accounts>>(Sv.findAll(),HttpStatus.OK);
	}
}
